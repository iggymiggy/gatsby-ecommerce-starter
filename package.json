{
  "name": "gatsby-starter-ecommerce",
  "private": false,
  "license": "MIT",
  "description": "Gatsby starter to create an ecommerce website with netlify and stripe.",
  "version": "0.1.0",
  "keywords": [
    "e-commerce",
    "gatsby",
    "firebase",
    "netlify",
    "stripe"
  ],
  "scripts": {
    "clean": "gatsby clean",
    "start": "run-p start:**",
    "start:app": "gatsby develop",
    "start:lambda": "netlify-lambda serve -c ./webpack.development.js src/functions",
    "build": "run-p build:**",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build -c ./webpack.production.js src/functions",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "catalog:create": "node ./scripts/createCatalog.js",
    "catalog:update": "node ./scripts/updateCatalog.js",
    "reset": "rm -rf ./node_modules && yarn install",
    "locales": "yarn run babel --config-file ./babel-extract.config.js -o tmp/chunk.js 'src/**/*.{js,jsx,ts,tsx}' && rm -rf tmp"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.6",
    "@reach/router": "^1.3.4",
    "@stripe/stripe-js": "^1.3.1",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "babel-plugin-styled-components": "^1.10.7",
    "clsx": "^1.0.4",
    "dotenv-webpack": "^2.0.0",
    "eslint-config-blvd": "^1.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslintrc-generator": "^1.0.2",
    "firebase": "^7.14.0",
    "gatsby": "^2.25.0",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-image": "^1.9.0",
    "gatsby-plugin-layout": "^1.2.1",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-material-ui": "^2.1.6",
    "gatsby-plugin-netlify": "^2.2.4",
    "gatsby-plugin-netlify-cms": "^4.2.5",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-react-i18next": "^0.0.27",
    "gatsby-plugin-sass": "^2.2.4",
    "gatsby-plugin-sharp": "^3.9.0",
    "gatsby-plugin-styled-components": "^3.2.1",
    "gatsby-remark-copy-linked-files": "^2.2.4",
    "gatsby-remark-images": "^3.2.6",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-source-stripe": "^3.0.7",
    "gatsby-transformer-remark": "^2.7.5",
    "gatsby-transformer-sharp": "^3.9.0",
    "i18next": "^19.5.1",
    "loglevel": "^1.7.1",
    "netlify-cli": "^3.37.37",
    "netlify-cms-app": "^2.14.47",
    "netlify-cms-media-library-cloudinary": "^1.3.10",
    "netlify-cms-media-library-uploadcare": "^0.5.10",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.0",
    "react-image-gallery": "^1.2.4",
    "react-player": "^2.9.0",
    "resize-observer-polyfill": "^1.5.0",
    "react-swipeable": "6.0.1",
    "slug": "^3.3.4",
    "stripe": "^8.38.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/plugin-transform-typescript": "^7.10.3",
    "babel-plugin-i18next-extract": "^0.7.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-root-import": "^2.0.5",
    "http-proxy-middleware": "^1.0.3",
    "netlify-lambda": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}